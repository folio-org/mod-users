{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Collection of user groups",
  "properties": {
    "usergroups": {
    "description": "List of user group items",
      "id": "groups",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "usergroup.json"
      }
    },
    "totalRecords": {
      "type": "integer",
      "description": "Estimated or exact total number of records, see https://github.com/folio-org/raml-module-builder#estimated-totalrecords for details"
    }
  }
}
