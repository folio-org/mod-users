{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Collection of address types",
  "properties": {
    "addressTypes": {
      "description": "List of address type items",
      "type": "array",
      "id": "addressType",
      "items": {
        "type": "object",
        "$ref": "addresstype.json"
      }
    },
    "totalRecords": {
      "type": "integer",
      "description": "Estimated or exact total number of records, see https://github.com/folio-org/raml-module-builder#estimated-totalrecords for details"
    }
  },
  "required": [
    "addressTypes",
    "totalRecords"
  ]
}
